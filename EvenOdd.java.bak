import java.util.Scanner;
class EvenOdd 
{
	public static void main(String[] args) 
	{
		System.out.println("Enter a number: ");
		int no = new Scanner(System.in).nextInt();
		System.out.println( (no/2) == (no/2.0) );
		System.out.println( (no/2)*2 == no );
		
		/*
		other option:
		(no/2)*2 == no
		*/
		
		
		
		
	}
}
